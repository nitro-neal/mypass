{
  "extends": "strongloop",

  //   "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },

  "rules": {
    "quotes": [2, "double"],
    "strict": [0, "safe"],
    "space-before-function-paren": [0, "never"],
    "max-len": [2, 120, 8],
    "comma-dangle": [0, "never"],
    "eqeqeq": [0, "never"]
  },

  "env": { "es6": true }
}
